class Compute {
    
    public static int missingNumber(int A[], int N)
    {
         // Your code goes here
         int sum =0;
         int total_sum= (N*(N+1))/2;
         int a;
         if(N==0 || N==1){
             
         return N;
         }
         for(int i =0 ; i<A.length;i++){
             
             sum = sum+A[i];
         }
         a = (total_sum)-(sum);
         return a;
    }
}
