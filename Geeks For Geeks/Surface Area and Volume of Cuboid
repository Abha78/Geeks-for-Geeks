class Solution
{
    public long[] find(int l, int b, int h)
    {
        // code here 
        // 
        long L = l;
        long B = b;
        long H = h;
        
        long [] total= new long[2];
         total[0] = 2*(L*B+B*H+H*L);
         total[1] = L*B*H;
        return total;
        
    }
}
