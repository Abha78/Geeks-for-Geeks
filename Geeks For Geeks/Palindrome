class Solution
{
    public String is_palindrome(int n)
    {
        // Code here 
        int num = n;
      int rev =0;
        int rem=0 ;
        while(n!=0){ 
            rem=n%10; 
            rev = rev*10+rem ;
            n = n/10;
            
        }
        if(rev==num ){
            return "Yes";
        }
        //else {
            return "No";
        
       // return rev;
    }
}
